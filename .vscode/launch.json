{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch EthioCrown",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/x64/Debug/EthioCrown.exe", // Adjust the path to your executable
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build",
      "miDebuggerPath": "C:\\msys64\\ucrt64\\bin\\gdb.exe", // Adjusted the path to your gdb executable
      "miDebuggerArgs": "",
      "logging": {
        "moduleLoad": false,
        "trace": false,
        "engineLogging": false,
        "programOutput": true,
        "exceptions": true
      }
    }
  ]
}
